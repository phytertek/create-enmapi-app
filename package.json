    "create-enmapi-app": "bin/index.js"
  },
  "scripts": {
    "patch":
      "git add . && git commit -m 'publish patch' && npm run patch:publish",
    "patch:publish": "npm version patch && npm publish",
    "minor":
      "git add . && git commit -m 'publish minor' && npm run minor:publish",
    "minor:publish": "npm version minor && npm publish",
    "major":
      "git add . && git commit -m 'publish major' && npm run major:publish",
    "major:publish": "npm version major && npm publish"
  },
  "dependencies": {
    "cli-spinner": "^0.2.7",
    "js-beautify": "^1.7.5"
  }
}
